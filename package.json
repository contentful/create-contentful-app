{
  "name": "@contentful/create-contentful-app",
  "description": "A template for building Contentful Apps",
  "author": "Contentful GmbH",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/create-contentful-app.git"
  },
  "scripts": {
    "test": "nx run-many -t test",
    "lint": "nx run-many -t lint",
    "build": "nx run-many -t build",
    "prepare-release": "nx release --verbose --skip-publish",
    "prepare-release:canary": "nx release --preid alpha --verbose --skip-publish",
    "publish-packages": "nx release publish --verbose",
    "publish-packages:canary": "nx release publish --tag canary --verbose",
    "publish-packages:dry-run": "nx release publish --dry-run",
    "pre-commit": "nx run-many -t pre-commit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@nx/js": "22.4.1",
    "@types/mocha": "10.0.10",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-eslint": "10.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-node": "11.1.0",
    "fs-extra": "11.3.3",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "mocha": "11.7.5",
    "nx": "22.4.1",
    "prettier": "2.8.8",
    "proxyquire": "2.1.3",
    "sinon": "21.0.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "open": "^10.1.0"
  }
}
