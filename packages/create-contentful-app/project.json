{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "create-contentful-app",
  "projectType": "library",
  "sourceRoot": "packages/create-contentful-app",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/create-contentful-app"
      }
    },
    "pre-commit": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run pre-commit",
        "cwd": "packages/create-contentful-app"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "npm publish --access public",
        "cwd": "packages/create-contentful-app"
      },
      "configurations": {
        "dry-run": {
          "command": "npm publish --access public --dry-run"
        }
      }
    }
  },
  "implicitDependencies": ["@contentful/create-contentful-app"]
}

