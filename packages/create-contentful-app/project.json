{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "create-contentful-app",
  "projectType": "library",
  "sourceRoot": "packages/create-contentful-app",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/create-contentful-app"
      }
    },
    "pre-commit": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run pre-commit",
        "cwd": "packages/create-contentful-app"
      }
    },
    "nx-release-publish": {
      "options": {
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "implicitDependencies": ["@contentful/create-contentful-app"]
}

