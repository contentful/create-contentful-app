{
  "name": "@contentful/create-contentful-app",
  "version": "1.1.0-alpha.6",
  "description": "A template for building Contentful Apps",
  "keywords": [
    "contentful",
    "create-contentful-app"
  ],
  "author": "Contentful GmbH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/create-contentful-app.git",
    "directory": "packages/contentful--create-contentful-app"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6"
  },
  "main": "lib/index.js",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "bin": {
    "create-contentful-app": "lib/index.js"
  },
  "scripts": {
    "prettier": "prettier **/*.js --write --ignore-path .gitignore",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "build": "tsc"
  },
  "dependencies": {
    "@contentful/app-scripts": "file:../contentful--app-scripts",
    "chalk": "^4.1.0",
    "commander": "^9.0.0",
    "degit": "2.8.4",
    "inquirer": "^8.2.1",
    "rimraf": "^3.0.2",
    "tildify": "^2.0.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/contentful/create-contentful-app/issues"
  },
  "homepage": "https://github.com/contentful/create-contentful-app#readme",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/chalk": "^2.2.0",
    "@types/degit": "^2.8.3",
    "@types/inquirer": "^8.2.0",
    "@types/node": "^14.14.31",
    "@types/rimraf": "^3.0.2",
    "@types/tildify": "^2.0.2",
    "@types/validate-npm-package-name": "^3.0.3"
  }
}
