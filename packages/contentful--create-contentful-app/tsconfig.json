{
  "extends": "@tsconfig/node16/tsconfig.json",
  "exclude": ["node_modules", "**/*.spec.ts", "app-actions"],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "typeRoots": ["./typings", "./node_modules/@types"]
  }
}
