{
  "name": "@contentful/app-scripts",
  "version": "1.17.0",
  "description": "A collection of scripts for building Contentful Apps",
  "author": "Contentful GmbH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/create-contentful-app.git",
    "directory": "packages/contentful--app-scripts"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "bin": {
    "contentful-app-scripts": "lib/bin.js"
  },
  "scripts": {
    "prettier": "prettier **/*.js --write --ignore-path .gitignore",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha \"./{,!(node_modules)/**/}*.test.js\" --exit",
    "test:watch": "npm t -- --watch",
    "pre-commit": "lint-staged",
    "build": "rm -rf lib && tsc"
  },
  "bugs": {
    "url": "https://github.com/contentful/create-contentful-app/issues"
  },
  "homepage": "https://github.com/contentful/create-contentful-app#readme",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@segment/analytics-node": "^2.0.0",
    "adm-zip": "0.5.12",
    "bottleneck": "2.19.5",
    "chalk": "4.1.2",
    "commander": "12.0.0",
    "contentful-management": "11.24.0",
    "dotenv": "16.4.5",
    "ignore": "5.3.1",
    "inquirer": "8.2.6",
    "lodash": "4.17.21",
    "open": "8.4.2",
    "ora": "5.4.1"
  },
  "gitHead": "4c3506be3f52c7a8aae17deaa75acefc9a805b42",
  "devDependencies": {
    "@tsconfig/node18": "18.2.2",
    "@types/adm-zip": "0.5.5",
    "@types/analytics-node": "3.1.14",
    "@types/inquirer": "8.2.1",
    "@types/lodash": "4.17.0",
    "@types/proxyquire": "1.3.31",
    "@types/sinon": "17.0.3",
    "proxyquire": "2.1.3",
    "sinon": "17.0.1",
    "ts-node": "10.9.2"
  }
}
