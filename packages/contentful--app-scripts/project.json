{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@contentful/app-scripts",
  "projectType": "library",
  "sourceRoot": "packages/contentful--app-scripts/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/contentful--app-scripts"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
