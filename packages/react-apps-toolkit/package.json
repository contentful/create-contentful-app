{
  "name": "react-apps-toolkit",
  "version": "0.0.1",
  "description": "Toolkit for building a Contentful app in React",
  "keywords": [],
  "author": "Contentful GmbH",
  "license": "MIT",
  "main": "dist/index.js",
  "bugs": {
    "url": "https://github.com/contentful/create-contentful-app/issues"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
